{% macro hexlify(char) %}
{{- char.encode().hex() -}}
{% endmacro %}
{% macro uuid(str) %}
{{- hexlify(str[:4]) }}-{{ hexlify(str[4:6]) }}-{{ hexlify(str[6:8]) }}-{{ hexlify(str[8:10]) }}-{{ hexlify(str[10:16]) -}}
{% endmacro %}
{
  "WARNING": "This file is automatically generated by Ansible. Do not edit it manually unless you know what you are doing. Removing this file will cause act runner to re-register as a new runner.",
  "id": 0,
  "uuid": "{{ uuid(forgejo_runner_secret[:16]) }}",
  "name": "{{ forgejo_runner_name }}",
  "token": "{{ forgejo_runner_secret }}",
  "address": "{{ forgejo_runner_for }}",
  "labels": {{ forgejo_runner_config.runner.labels | ansible.builtin.to_json(indent=2) | indent(2) }}
}
